{
  "name": "nextjs-azure-function",
  "version": "1.0.11",
  "description": "Builds your NextJS /api pages/api folder into a single azure function",
  "main": "index.js",
  "scripts": {
    "build": "tsc ./src/api-handler.ts --skipLibCheck true && tsc ./src/index.ts --skipLibCheck true"
  },
  "bin": {
    "nextjs-azure-function": "src/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShanonJackson/nextjs-azure-function.git"
  },
  "keywords": [
    "NextJS",
    "Azure",
    "Azure",
    "Function"
  ],
  "author": "Shanon Jackson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShanonJackson/nextjs-azure-function/issues"
  },
  "homepage": "https://github.com/ShanonJackson/nextjs-azure-function#readme",
  "devDependencies": {
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@azure/functions": "^1.2.3",
    "@sls-next/lambda-at-edge": "^1.6.0-alpha.9",
    "fs-extra": "^9.1.0",
    "next": "^10.0.6"
  }
}
